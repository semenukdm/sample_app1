<% provide(:title, 'upload') %>
<h1>UPLOAD X3D</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(User.name) do |f| %>
     
      <%= f.label :x3d %>
      <%= f.file_field :name, class: 'form-control' %>

      <%= f.label :textures %>
      <%= f.file_field :email, class: 'form-control' %>

      
      <%= f.submit "Upload", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>